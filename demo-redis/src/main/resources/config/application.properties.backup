# This configuration will be overriden by the Spring profile you use,
# for example application-{dev|stg|prod}.properties if you use the "dev","stg",'prod' profile.
# e.g.: --spring.profiles.active=dev

spring.application.name=demoRedis-properties
app.log.path=d:/logs/
app.log.level=debug


# redis 数据库索引（默认为0）
spring.redis.database=0
spring.redis.host=192.168.20.81
spring.redis.port=6379
spring.redis.password:
spring.redis.timeout=1000
# 连接池最大连接数（使用负值表示没有限制）
spring.redis.pool.maxActive=200
# 连接池最大阻塞等待时间（使用负值表示没有限制）
spring.redis.pool.maxWait=-1
# 连接池中的最大空闲连接
spring.redis.pool.maxIdle=10
# 连接池中的最小空闲连接
spring.redis.pool.minIdle=0